*

#include<stdio.h>
int main(){
    for (int i=1;i<5;i++){
        for(int j=1;j<=i;j++){
            printf("*");
        }
        printf("\n");
    }
    for (int i=4;i>0;i--){
        for(int j=i;j>0;j--){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


Type number of rows9
        *
       **
      ***
     ****
    *****
   ******
  *******
 ********
*********

#include<stdio.h>
int main(){
    int num;
    printf("Type number of rows");
    scanf("%d",&num);
    for(int i=1;i<=num;i++){
        for(int j=1;j<=num-i;j++){
            printf(" ");
        }
        for(int m=1;m<=i;m++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


